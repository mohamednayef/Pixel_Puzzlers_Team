//https://codeforces.com/problemset/problem/935/A
#include <iostream>
#include <string>
using namespace std;
 
int main(){
    int n;
    cin >> n;
    int result = 1;  // 2
    
    for(int i=2; i<=n/2; i++){ // 3
        if( n%i == 0 ){
            result++;
        }
    }
    cout << result << endl;
    
    
    
}
